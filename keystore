keytool -genkey -alias mycertificate -keystore mycertificate.p12 -storepass mypassword -validity 10000 -keyalg RSA -keysize 2048 -storetype pkcs12


  
tns build android --release --key-store-path mycertificate.p12 --key-store-password mypassword --key-store-alias mycertificate --key-store-alias-password mypassword

